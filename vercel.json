{
  "version": 2,
  "name": "medcore-auth-service",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3002",
    "DATABASE_URL": "@database-url",
    "JWT_SECRET": "@jwt-secret",
    "SMTP_HOST": "@smtp-host",
    "SMTP_PORT": "@smtp-port",
    "SMTP_USER": "@smtp-user",
    "SMTP_PASS": "@smtp-pass",
    "SMTP_SECURE": "@smtp-secure",
    "AUDIT_SERVICE_URL": "@audit-service-url"
  }
}